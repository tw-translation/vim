msgid "-b\t\t\tBinary mode"
msgstr "-b\t\t\tДвоичный режим"

#: main.c:3515
msgid "-l\t\t\tLisp mode"
msgstr "-l\t\t\tРежим Lisp"

#: main.c:3517
msgid "-C\t\t\tCompatible with Vi: 'compatible'"
msgstr "-C\t\t\tРежим совместимости с Vi: 'compatible'"

