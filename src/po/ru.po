msgstr "-s\t\t\tТихий (пакетный) режим (только для \"ex\")"

#: main.c:3506
msgid "-d\t\t\tDiff mode (like \"vimdiff\")"
msgstr "-d\t\t\tРежим отличий (как \"vimdiff\")"

